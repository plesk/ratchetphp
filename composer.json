    "php": "^8.0",
    "ratchet/rfc6455": "^0.4.0",
    "react/socket": "^1.13.0",
    "react/event-loop": "^1.4.0",
    "guzzlehttp/psr7": "^2.5.0",
    "symfony/http-foundation": "^5.4.24 || ^6.0 || ^7.0",
    "symfony/routing": "^5.4.22 || ^6.0 || ^7.0",
    "nyholm/psr7": "^1.8"
  },
  "config": {
    "platform": {
      "php": "8.0.0"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.9",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs"
  }
