{
  "name": "plesk/ratchetphp",
  "type": "library",
  "description": "PHP WebSocket library",
  "keywords": [
    "WebSockets",
    "Server",
    "Ratchet",
    "Sockets",
    "WebSocket"
  ],
  "homepage": "http://socketo.me",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Boden",
      "email": "cboden@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Matt Bonneau",
      "role": "Developer"
    },
    {
      "name": "Alexey Yuzhakov",
      "role": "Developer"
    }
